<LinearLayout
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/header"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/header_pqrregister"
        android:layout_gravity="center" />
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_gravity="center">
        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/imgPQR" />

        </ScrollView>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtResult" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnResult"
        android:layout_gravity="center" />

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/webTwitter"
        android:layout_gravity="center" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_pqrregisterback"
        android:id="@+id/btnBack"
        android:layout_gravity="center" />
</LinearLayout><!-- From: file:/C:/Users/Cristian%20Cantillo/AndroidStudioProjects/TramiteFacil/TramiteFacil/src/main/res/layout/activity_pqrregister.xml -->