package com.bpmco.tramitefacil;

import android.content.Context;
import android.content.Intent;
import android.widget.Button;

/**
 * Created by Cristian Cantillo on 1/01/14.
 */
public class FooterTab
{
    public static void accionFooter(Context c, Button btnFooter){
        Intent i = null;
        if(btnFooter.getId() == R.id.tab1){
            i = new Intent(c, Activity_qdSaber.class);
        }else  if(btnFooter.getId() == R.id.tab2){
            i = new Intent(c, Activity_qdSaber.class);
        }else  if(btnFooter.getId() == R.id.tab3){
            i = new Intent(c, Activity_qdSaber.class);
        }else{
            i = new Intent(c, Activity_qdSaber.class);
        }
        c.startActivity(i);
    }
}
