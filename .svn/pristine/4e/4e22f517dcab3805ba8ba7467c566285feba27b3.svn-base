<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="0dp"
    android:background="@color/bar_theme_released_end"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/header"
        android:paddingLeft="0dp"
        android:orientation="horizontal"
        >

        <Button
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text="Atras"
            android:textAlignment="center"
            android:gravity="center"
            android:id="@+id/btnBack"
            android:textSize="14dp"
            style="@style/textoEncabezado"
            android:background="@drawable/botonheader"/>

        <TextView
            android:id="@+id/txtcabecera"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:layout_marginLeft="@dimen/activity_vertical_margin"
            android:text="Alcaldia de Manizales"
            android:textSize="22sp"
            android:ellipsize="marquee"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:lines="1"
            style="@style/textoEncabezado"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text="Inicio"
            android:id="@+id/btnHome"
            android:gravity="center"
            android:textAlignment="center"
            android:textSize="14dp"
            style="@style/textoEncabezado"
            android:background="@drawable/botonheader"/>
    </LinearLayout>
</LinearLayout><!-- From: file:/C:/Users/Cristian%20Cantillo/AndroidStudioProjects/TramiteFacil/TramiteFacil/src/main/res/layout/header_listado_pqr.xml -->